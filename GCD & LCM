#include<stdio.h>
int main()
{
    int a,b,gcd, lcm;
    printf("Enter 2 numbers : ");
    scanf("%d %d" , &a, &b);
    
    for (int i = 1; i <=b; i++)
    {
        if( a % i == 0 && b % i == 0)
        {
            gcd = i;
        }
    }
    lcm = (a*b)/gcd;
    
    printf("Gcd = %d\n" , gcd);
    printf("Lcm = %d\n" , lcm);
    return 0;
}
