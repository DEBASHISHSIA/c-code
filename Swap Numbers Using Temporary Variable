#include<stdio.h>
int main()
{
    int a,b,temp;
    printf("Enter a number : ");
    scanf("%d" , &a);
    printf("Enter a number : ");
    scanf("%d" , &b);
    
    printf("Before swapping \n");
    printf("A = %d & B = %d\n", a,b );
    temp = a;
    a = b;
    b = temp;
    printf("After swapping \n");
    printf("A = %d & B = %d\n", a,b );
    return 0;
}
