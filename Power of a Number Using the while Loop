#include<stdio.h>
int main()
{
    int base , exponent ;
    long long result = 1;
    printf("Enter a base number : ");
    scanf("%d" , &base);
    printf("Enter exponent : ");
    scanf("%d" , &exponent);
    while ( exponent != 0)
    {
        result = result*base;
        exponent--;
    }
    printf("ANS = %lld", result);
    return 0;
}
