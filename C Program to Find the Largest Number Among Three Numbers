#include<stdio.h>
int main()
{
    int a,b,c;
    printf("Enter 3 number : ");
    scanf("%d %d %d" , &a,&b,&c );
    if (a > b && a > c)
    {
        printf("A is the largest number & it's value is : %d" , a);
    }
    if ( b > c && b > a)
    {
        printf("B is the largest number & it's value is : %d" , b);
    }
    if (c > a && c > b)
    {
        printf("C is the largest number & it's value is : %d" , c);
    }
    return 0;
}
