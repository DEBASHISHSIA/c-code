#include<stdio.h>
int main()
{
    int n, prime = 0;
    printf("Enter a number : ");
    scanf("%d" , &n);
    for (int i = 2; i < n; i++)
    {
        if ( n % i == 0 )
        {
            prime ++;
            break;
        }
    }
    if (prime == 1)
    {
        printf("This is not aprime number");
    }
    else
    {
    
        printf("This is a aprime number");  
    }
    return 0;
}
