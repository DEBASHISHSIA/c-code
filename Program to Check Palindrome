#include<stdio.h>
int main()
{
    int n , d , r = 0 , temp;
    printf("Enter a number : ");
    scanf("%d" , &n);
    temp = n;
    while ( n > 0)
    {
        d = n % 10;
        r = (r * 10) + d;
        n = n/10;
    }
    if ( temp == r)
    {
        printf("This is palimfrome number");
    }
    else
    {
        printf("This is not a palimfrome number");
    }
    return 0;
}
