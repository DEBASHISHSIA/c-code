#include<stdio.h>
int main()
{
    int row, col, i, j, arr[10][10], count = 0;
    printf("Enter row : \n");
    scanf("%d" , &row);
    printf("Enter column : \n");
    scanf("%d" , &col);
    printf("Enter matrix elemetnts : \n");
    for (i = 0; i < row; i++)
    {
        for (j = 0; j <col; j++)
        {
            scanf("%d" , &arr[i][j]);
        }
    }
    printf("Martix elements are : \n");
    {
        for (i = 0; i <row; i++)
        {
            for (j= 0 ; j < col; j++)
            {
                printf("%d\t" , arr[i][j]);
            }
            printf("\n");
        }
    }
    // checking sparse matrix or not
    for (i = 0; i < row; i++)
    {
        for (j = 0; j < col ;j++)
        {
            if ( arr[i][j] == 0 )
            {
                count++;
            }
        }
    }
    if ( count > (row*col)/2 )
    {
        printf("This is a sparse matrix\n");
    }
    else
    {
        printf("This is not a sparse matrix\n");
    }
    
    return 0;
}
